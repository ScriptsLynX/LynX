local function notify(text,time)
	game.StarterGui:SetCore("SendNotification", {
	Title = "LynX";
	Text = text;
	Duration = time;
	Callback = NotificationBindable;
	})
end

if game.PlaceId == 294790062 then
  loadstring(game:HttpGet("https://raw.githubusercontent.com/ScriptsLynX/LynX/main/RobloxTitanic"))()
elseif game.PlaceId == 2539588846 or game.PlaceId == 2306562216 then  
  loadstring(game:HttpGet("https://raw.githubusercontent.com/ScriptsLynX/LynX/main/Camping"))()
elseif game.PlaceId == 3199109640 or game.PlaceId == 3109123524 then
  loadstring(game:HttpGet("https://raw.githubusercontent.com/ScriptsLynX/LynX/main/Camping2"))()
elseif game.PlaceId == 3390084922 or game.PlaceId == 3378223169 then
  loadstring(game:HttpGet("https://raw.githubusercontent.com/ScriptsLynX/LynX/main/Hotel"))()
else
notify("No script was found for this game!",5)
end
